#include <stdio.h>
#include<string.h>

int main()
{
    	char s[20],m[20];
	int i,j,size,count=0;
	printf("Enter a string");
	gets(s);//welcome
	for(i=0;s[i]!=0;i++)
	{
	    m[i]=s[i];
	}
	size=strlen(s);
	i=0; j=size-1;
	while(i<(size/2))
	{
		char temp;
		temp=s[i]; //0  --- w
		s[i]= s[j];   // e......?
		s[j]=temp; //w 
		i++;
		j--;
	}
	for(i=0;s[i]!='\0';i++)
	{
	    if(s[i]==m[i]) 
	    {
	        count=count+1;
	    }
	   
	}
	if(count==size)
	printf("palindrome");
	else
	printf("not palindrome");

    return 0;
}
